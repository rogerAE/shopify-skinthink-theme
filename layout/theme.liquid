<!doctype html>
<html class='no-js' lang='{{ request.locale.iso_code }}' dir='{{ settings.text_direction }}'>
  <head>
{% if template.name contains 'product' %}
  <script src="https://cdn.loox.io/widget/loox.min.js" async></script>
{% endif %}


    
