          "type": "liquid",
          "settings": {
            "code": "<div class=\"heading\" style=\"display: flex; justify-content: center; align-items: center; gap: 10px; text-align: center;\">\n  <h2 style=\"margin: 0;\">A NOTE FROM OUR FOUNDER</h2>\n</div>",
